{
  "name": "template-react",
  "version": "0.0.1",
  "private": true,
  "main": "webpack.config.js",
  "engines": {
    "node": ">=13.0",
    "npm": ">=6.0",
    "yarn": ">=1.20"
  },
  "scripts": {
    "precommit": "yarn lint",
    "prepush": "lint-staged",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing!')\"",
    "start": "npx solidarity && cross-env webpack-dev-server --config webpack.config.js --mode development",
    "start:mock": "yarn mockserver && cross-env webpack-dev-server --config webpack.config.mock.js --mode development",
    "build": "npx solidarity && rm -rf ./public/dist && cross-env && webpack --config webpack.config.production.js --mode production && rm -rf ./dist && mv ./public/dist ./dist",
    "preview": "yarn build && cross-env NODE_ENV=preview http-server ./public/dist -p 1234 -c-1",
    "clean": "rm -rf ./node_modules && yarn install",
    "test": "cross-env NODE_ENV=test echo \"Error: no test specified\" && exit 1",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "tsc && eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix --max-warnings 100",
    "mockserver": "yarn json-server ./_mock/mock.json --routes ./_mock/routes.json -w -p 3001 -d 500 &",
    "killserver": "kill -9 $(lsof -ti:3001)"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn pretty",
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "@platformbuilders/helpers": "^0.3.2",
    "@platformbuilders/react-elements": "^0.0.4",
    "@platformbuilders/validations": "^0.0.8",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "cpf_cnpj": "^0.2.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "favicons-webpack-plugin": "^4.2.0",
    "firebase": "^8.0.1",
    "formik": "^2.2.5",
    "history": "4.10.1",
    "i18n-js": "^3.8.0",
    "lazy-route": "^1.0.7",
    "lodash": "^4.17.20",
    "lottie-web": "^5.7.4",
    "mobx": "5.15.7",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.3.1",
    "mobx-react-router": "^4.1.0",
    "mobx-state-tree": "^3.17.2",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-input-mask": "^2.0.4",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.1.0",
    "reactotron-mst": "^3.1.3",
    "reactotron-react-js": "^3.3.7",
    "rfx-core": "^1.6.2",
    "styled-components": "^5.2.1",
    "whatwg-fetch": "^3.5.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
