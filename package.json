    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.6.3",
    "json-server": "^0.16.2",
    "lint-staged": "^10.5.1",
